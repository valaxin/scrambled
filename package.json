{
  "name": "scrambled",
  "description": "Lightweight ",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "author": {
    "name": "valaxin",
    "url": "https://github.com/valaxin"
  },
  "bugs": {
    "url": "https://github.com/valaxin/scrambled/issues"
  },
  "type": "module",
  "scripts": {
    "clean": "rm -rf ./.web-temp/* ./.bot-temp/*",
    "start:bot": "nodemon -i ./config.js -i ./App/www/* -w ./App/src/* --verbose --trace-warnings --experimental-json-modules ./app/src/index.js",
    "start:web": "nodemon -i ./config.js -i ./App/src/* -w ./App/www/* --verbose --trace-warnings ./app/www/index.js",
    "start": "npm run clean && concurrently 'npm run start:web' 'npm run start:bot'"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "axios": "^1.6.0",
    "body-parser": "^1.20.2",
    "chalk": "^5.0.1",
    "cheerio": "^1.0.0-rc.12",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "discord.js": "^14.14.1",
    "express": "^4.18.2",
    "ffmpeg": "0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "morgan": "^1.10.0",
    "node-fetch": "^3.1.1",
    "nodemon": "~3.1.0",
    "socket.io": "^4.7.2",
    "ytdl-core": "^4.9.2"
  }
}
