{
  "name": "scrambled",
  "description": "~",
  "version": "0.1.1",
  "main": "index.js",
  "license": "MIT",
  "author": "valaxin",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run bot:dev\" \"npm run web:dev\"",
    "bot:dev": "nodemon --watch app/bot --verbose --trace-warnings app/bot/main.js",
    "bot:run": "node ./app/bot/main.js",
    "bot:reg": "node ./app/bot/support/_registerCommands.js",
    "bot:del": "node ./app/bot/support/_removeRegisteredCommands.js",
    "web:dev": "nodemon --watch app/web --ignore '/app/web/db/*.*' --verbose --trace-warnings app/web/server.js"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "bufferutil": "^4.0.8",
    "concurrently": "^8.2.2",
    "discord.js": "^14.14.1",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "nodemon": "^3.1.0",
    "rss-parser": "^3.13.0",
    "utf-8-validate": "^6.0.3",
    "zlib-sync": "^0.1.9"
  }
}
