{
  "name": "scrambled",
  "description": "personal discord bot",
  "version": "0.1.5",
  "main:bot": "bot/index.js",
  "main:web": "server/app.js",
  "license": "MIT",
  "private": "true",
  "author": "valaxin",
  "type": "module",
  "scripts": {
    "start": "conc \"npm run start:web\" \"npm run bot:deploy & npm run dev:bot\"",
    "start:bot": "node bot/client.js",
    "start:web": "node server/app.js",
    "bot:deploy": "node bot/deploy.js",
    "bot:remove": "node bot/remove.js",
    "dev:web": "nodemon --watch server --exec node server/app.js",
    "dev:bot": "nodemon --watch bot --exec node bot/client.js",
    "dev": "conc \"npm run dev:web\" \"npm run dev:bot\""
  },
  "dependencies": {
    "axios": "^1.12.0",
    "body-parser": "^2.2.0",
    "bufferutil": "^4.0.9",
    "concurrently": "^9.2.1",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "mongoose": "^8.18.1",
    "morgan": "^1.10.1",
    "nodemon": "^3.1.10",
    "socket.io": "^4.8.1",
    "utf-8-validate": "^6.0.5"
  }
}
