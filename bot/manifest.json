          "required": false,
          "default": "hot",
          "selection": [ "hot", "top", "new" ]
        }
      }
    },
    "stream": {
      "ephemeral": true,
      "description": "",
      "arguments": {
        "type": {},
        "title": {},
        "season": {},
        "episode": {}
      }
    },
    "about": {
      "ephemeral": true,
      "description": "returns information about this bot"
    },
    "alarm": {}
  }
}